all: librlundoable rlundo test
librlundoable: rlundoable.c
	gcc -c rlundoable.c
	gcc -dynamiclib -current_version 1.0 rlundoable.o -o librlundoable.dylib
	gcc rlundoable.c -lreadline -o testrlundoable
rlundo: rlundo.c 
	gcc rlundo.c -o rlundo
test.out: test.c rlunduable
	gcc -c test.c
	gcc -v test.o ./librlundoable.dylib -o test.out
